<h1>Login Page</h1>

<%if flash[:error]%>
<%=flash[:error]%>
<%end%>



<%=form_tag login_path do %>
<br>
<label>Email:</label>
<%=text_field_tag :email%>
<label>Password: </label>
<%=password_field_tag :password%>
<%=submit_tag "Login"%>
<%end%>

<br>
<p>No existing account? <%=link_to "Sign Up", users_path%><p>


